#include "base/common.h"

void initColors(){
	init_pair(1, COLOR_RED, COLOR_BLACK);
	init_pair(2, COLOR_CYAN, COLOR_BLACK);
	init_pair(3, COLOR_GREEN, COLOR_BLACK);
}

int main(){

	int input;

	srand(time(NULL));
	initW();
	if(colorsAble)initColors();
	getmaxyx(stdscr, maxY, maxX);



	endW();
	
	return 0;
}
